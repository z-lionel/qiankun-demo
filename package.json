{
  "name": "mainvue3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install:all": "yarn && cd examples/vueapp && yarn && cd ../../examples/reactapp && yarn",
    "start:all": "npm-run-all --parallel app:*",
    "app:main": "vue-cli-service serve",
    "app:vue": "cd examples/vueapp && yarn start",
    "app:react": "cd examples/reactapp && yarn start"
  },
  "dependencies": {
    "babel-loader": "8.1.0",
    "core-js": "^3.6.5",
    "install": "^0.13.0",
    "npm-run-all": "^4.1.5",
    "qiankun": "^2.5.1",
    "vue": "^3.2.22",
    "vue-router": "^4.0.12",
    "webpack": "4.44.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "7.11.0",
    "eslint-plugin-vue": "^7.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
